{
	"name": "server",
	"version": "0.0.2",
	"scripts": {
		"audit-ci": "audit-ci --moderate --report-type full --package-manager npm",
		"format": "prettier ./**/*.{gql,graphql,json,js,ts,tsx} -l",
		"format:fix": "npm run format -- --write",
		"generate": "graphql-codegen",
		"lint": "eslint ./src/**/*.{ts,tsx}",
		"lint:fix": "npm run lint -- --fix",
		"server:build:dev": "webpack --env.development",
		"server:build:prod": "webpack --env.production",
		"server:dev": "npm run server:build:dev && npm start",
		"server:watch": "nodemon",
		"sls:dev": "node --max-old-space-size=4096 node_modules/serverless/bin/serverless offline start",
		"start": "node .webpack/main",
		"storybook": "start-storybook -p 6006",
		"storybook:build": "build-storybook",
		"test": "jest",
		"test:coverage": "npm run test -- --coverage",
		"test:ci": "npm run audit-ci && npm run format && npm run lint",
		"tsnode": "ts-node -r tsconfig-paths/register --files ."
	},
