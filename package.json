{
	"name": "server",
	"version": "0.0.2",
	"description": "Serverless webpack example using Typescript",
	"main": "handler.js",
	"scripts": {
		"audit-ci": "audit-ci --moderate --report-type full --package-manager npm",
		"format": "prettier ./**/*.{gql,graphql,json,js,ts,tsx} -l",
		"format:fix": "npm run format -- --write",
		"generate": "graphql-codegen",
		"lint": "tslint --outputAbsolutePaths --project .",
		"lint:fix": "npm run lint -- --fix",
		"server:nodemon": "nodemon --exec 'node -r dotenv/config .webpack/main' dotenv_config_path=.env.development",
		"server:build:dev": "webpack --env.development",
		"server:build:prod": "webpack --env.production",
		"server:watch": "npm run server:build:dev && npm-run-all -p server:nodemon server:build:dev -- --watch",
		"sls:dev": "node --max-old-space-size=4096 node_modules/serverless/bin/serverless offline start",
		"start": "node .webpack/main",
		"storybook": "start-storybook -p 6006",
		"storybook:build": "build-storybook",
		"test": "jest",
		"test:coverage": "npm run test -- --coverage",
		"test:ci": "npm run audit-ci && npm run format && npm run lint"
	},
